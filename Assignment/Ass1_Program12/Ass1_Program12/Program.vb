Imports System.Console

Module Program
    Sub Main()

        Dim a, b, c As Double

        Write("Enter 1st Value : ")
        a = ReadLine()

        Write("Enter 2nd Value : ")
        b = ReadLine()

        Write("Enter 3rd Value : ")
        c = ReadLine()

        WriteLine("Average Of 3 Number Is " & (a + b + c) / 3)

    End Sub
End Module
